{
  "name": "typescript",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "git@github.com:chao7150/typescript.git",
  "author": "Satoru Yokoyama <22640673+chao7150@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && tsc -p ./tsconfig.json",
    "clean": "rm -rf ./build/*",
    "commit": "cz",
    "debug": "run-s build start",
    "format": "prettier --write ./src",
    "lint": "eslint './src/**/*.ts'",
    "start": "node -r dotenv/config build/index.js",
    "start:db": "docker-compose -f docker-compose-local.yml up",
    "test": "jest",
    "test:post": "bash scripts/post.sh",
    "test:prisma:studio": "DATABASE_URL='postgresql://prisma:mysecretpassword@localhost:5432/weather?schema=public' prisma studio"
  },
  "devDependencies": {
    "@prisma/cli": "^2.12.1",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@prisma/client": "^2.12.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  }
}
